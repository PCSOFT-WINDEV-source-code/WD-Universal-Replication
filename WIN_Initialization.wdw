   -
     name : _Process
     internal_properties : 
     procedure_id : 1760449597926765641
     type_code : 14
     group : 3
     code : |1+
      // Summary: Process a step
      // Syntax:
      //_Process ([<nStep> is int])
      //
      // Parameters:
      //	nStep (integer): Step whose process must be performed
      // Return value:
      // 	None
      //
      PROCEDURE _Process(LOCAL nStep is int = gnCurrentStep)
      
      sMasterReplica is string
      sMovableReplica is string
      
      // According to the step
      SWITCH nStep
      
      	CASE cstPLANE_MasterReplica
      		// Create the home directory of the data
      		fMakeDir(pGlobal.gsMasterDirectory)
      		
      		// Connect to this location to create the HyperFileSQL database
      		HOpenConnection(pGlobal.gsConnectionName_Portugal)
      		HChangeConnection(CUSTOMER, pGlobal.gsConnectionName_Portugal)
      		
      		// Create the data file and insert records
      		HCreation(CUSTOMER)
      		CUSTOMER.LASTNAME = "BERNARD"
      		CUSTOMER.CUSTOMERINFO = "This Customer was initially entered in Portugal"
      		CUSTOMER.DATETIMEID = DateSys()+TimeSys()
      		HAdd(CUSTOMER)
      		
      		Multitask(100)
      		CUSTOMER.LASTNAME = "CONSTANTIN"
      		CUSTOMER.CUSTOMERINFO = "This Customer was initially entered in Portugal"
      		CUSTOMER.DATETIMEID = DateSys()+TimeSys()
      		HAdd(CUSTOMER)
      		
      		// Create the master replica with this data
      		IF HCreateMasterReplica(pGlobal.gsMasterDirectory+["\"]+cstNameMasterReplica) = False THEN 
      			EndProgram("Failure creating the master replica." +CR+ HErrorInfo(hErrMessage))
      		END
      		
      	CASE cstPLANE_SubscriberReplica
      		
      		// Create the home directory of the data
      		fMakeDir(pGlobal.gsSubscriberDirectory)
      		
      		// Create the subscriber replica for the site
      		IF HCreateSubscriberReplica(pGlobal.gsMasterDirectory, pGlobal.gsSubscriberDirectory, "SPAIN", 0, "CUSTOMER" +TAB+ "DATETIMEID") = False THEN
      			EndProgram("Failure creating the subscriber replica." +CR+ HErrorInfo(hErrMessage))
      		END
      		
      	CASE cstPLANE_Transfer
      		
      		// Transfer the HyperFileSQL database into the ACCESS database (first synchronization)
      		// Then, the Access database can be exploited
      		// with these replication files
      		sMasterReplica = pGlobal.gsMasterDirectory +["\"]+ cstNameMasterReplica
      		
      		// Open a connection on the HyperFileSQL database (Portugal)
      		OpenAConnection("PORTUGAL")
      		
      		// Create the movable replica with the initial content of the HyperFileSQL database
      		HCreateMoveableReplica(sMasterReplica, "SPAIN", "")
      		
      		// Establish a connection on the Spanish Access database
      		OpenAConnection("SPAIN")
      		sMovableReplica = pGlobal.gsMasterDirectory + RPL.File
      		
      		// Create the data file: the Access database
      		HCreation(CUSTOMER)
      		
      		// Synchronize the replica with the Access database in order for the data of the master to be found in the subscriber
      		HSynchronizeReplica(sMovableReplica, pGlobal.gsSubscriberDirectory +["\"]+ cstNameMainSubscriberReplica, rplToSubscriber)
      		
      		// A synchronization must also be performed from the subscriber to the master
      		// The master will be "informed" that the initial synchronization was successfully performed
      		HCreateMoveableReplica(pGlobal.gsSubscriberDirectory +["\"]+ cstNameMainSubscriberReplica, "SPAIN", "")
      		sMovableReplica = pGlobal.gsTransferDirectory +["\"]+ RPL.File 
      		fCopyFile(pGlobal.gsSubscriberDirectory +["\"]+ RPL.File, sMovableReplica)
      		
      		// Connect to the master database in order to synchronize this new subscriber replica...
      		OpenAConnection("PORTUGAL")		
      		HSynchronizeReplica(pGlobal.gsMasterDirectory +["\"]+ cstNameMasterReplica, sMovableReplica, rplToMaster)
      		
      	CASE cstPLANE_MovableReplica
      		
      		// Create the directory to store the movable replicas
      		fMakeDir(pGlobal.gsTransferDirectory)
      		
      END
     type : 458752
   -
     name : NextStep
     procedure_id : 1760449597926831177
     type_code : 14
     code : |1-
      // Summary: goes to the next step
      // Syntax:
      //NextStep ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE NextStep()
      
      // Process the current step
      _Process()
      
      // Increments the current step
      gnCurrentStep++
      
      // Displays the step
      DisplayCurrentStep()
     type : 458752
