      // Summary: Displays the current step
      // Syntax:
      // DisplayCurrentStep ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DisplayCurrentStep()
      
      
      // Displays the text
      _DisplayText()
      
      // Displays the code
      _DisplayCode()
      
      // Displays the animation
      _DisplayAnimation()
      
      // If it si the last step, hides the "Next step" button
      IF gnCurrentStep = gnNumberStep THEN
      	BTN_NextStep..Visible = False
      END
     type : 458752
