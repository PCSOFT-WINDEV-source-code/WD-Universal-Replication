      PROCEDURE WIN_Initialization()
      
      // Constants
      CONSTANT
      	cstPLANE_MasterReplica = 1
      	cstPLANE_SubscriberReplica = 2
      	cstPLANE_Transfer = 3
      	cstPLANE_MovableReplica = 4
      END
      
      // Global variables
      gnCurrentStep is int = 0
      gnNumberStep is int = 4
      
      // Initialize the window
      Initialization()
