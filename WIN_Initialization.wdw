     name : Initialization
     procedure_id : 1760449597926437961
     type_code : 14
     code : |1+
      // Summary: Initialize the window
      // Syntax:
      //Initialization ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Initialization()
      
      sListDirectories is string
      sDirectory is string
      	
      	
      // Checks whether elements from a previous execution are found in order to delete them
      IF fDir(pGlobal.gsMasterDirectory, frDirectory) <> "" OR fDir(pGlobal.gsSubscriberDirectory, frDirectory) <> "" OR fDir(pGlobal.gsTransferDirectory, frDirectory) <> "" THEN
      	// Assigns a time-out before closing
      	DelayBeforeClosing(500)
      	// Asks confirmation to the user
      	IF YesNo("Data coming from a previous execution of the example was found." +CR+ "Do you want to delete it?" +CR+CR+ "Note: " +CR+ "Deleting the data of a previous execution is highly recommended: the initialization could trigger some errors if data is already found.") THEN 
      		// Close the Customer file
      		HClose(CUSTOMER)
      		// Close the connection
      		HCloseConnection(pGlobal.gsCurrentConnection)
      		// List of previous directories: master and subscriber(s)...
      		sListDirectories = fListDirectory(pGlobal.gsHomeDirectory, frNotRecursive)
      		// Deletes the listed directories
      		FOR EACH string sDirectory of sListDirectories SEPARATED BY CR
      			fRemoveDir(sDirectory, frRecursive)
      		END
      	END
      	// Deletes the time-out before closing
      	DelayBeforeClosing()
      END
      
     type : 458752
