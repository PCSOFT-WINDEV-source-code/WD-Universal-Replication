      // Summary: Displays the explanatory text of a step
      // Syntax:
      //_DisplayText ([<nStep> is int])
      //
      // Parameters:
      //	nStep (integer): Number of the step
      // Return value:
      // 	None
      //
      PROCEDURE _DisplayText(LOCAL nStep is int = gnCurrentStep)
      
      // According to the step
      SWITCH nStep
      
      	CASE cstPLANE_MasterReplica
      		STC_Step = "Creating the HyperFileSQL Classic data in Portugal (master replica)"
      		EDT_ExplanatorySteps = STC_Explanations_Step_1
      		
      	CASE cstPLANE_SubscriberReplica
      		STC_Step = "Creating the Access data of Spain (subscriber replica)"
      		EDT_ExplanatorySteps = STC_Explanations_Step_2
      		
      	CASE cstPLANE_Transfer
      		STC_Step = "Inserting data of the HyperFileSQL Classic database into the Access database"
      		EDT_ExplanatorySteps = STC_Explanations_Step_3
      		
      	CASE cstPLANE_MovableReplica
      		STC_Step = "Creating the transfer directory for synchronizing the data (movable replica)"
      		EDT_ExplanatorySteps = STC_Explanations_Step_4
      END
     type : 458752
