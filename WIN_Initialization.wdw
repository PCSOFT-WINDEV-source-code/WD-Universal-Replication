     code : |1-
      // Summary: Displays the animation of a step
      // Syntax:
      //_DisplayAnimation ([<nStep> is int])
      //
      // Parameters:
      //	nStep (integer): Number of the step
      // Return value:
      // 	None
      //
      PROCEDURE _DisplayAnimation(LOCAL nStep is int = gnCurrentStep)
      
      
      GR_AnimationControls..Visible = False
      // According to the step
      SWITCH nStep
      
      	CASE cstPLANE_MasterReplica
      		IMG_AnimationBackground = IMG_Step1
      		
      	CASE cstPLANE_SubscriberReplica
      		IMG_AnimationBackground = IMG_Step2
      		
      	CASE cstPLANE_Transfer
      		IMG_AnimationBackground = IMG_Step3
      		
      	CASE cstPLANE_MovableReplica
      		IMG_AnimationBackground = IMG_Step4
      		
      END
