     name : _DisplayCode
     procedure_id : 1760449597926634569
     type_code : 14
     group : 3
     code : |1-
      // Summary: Displays the code of a step
      // Syntax:
      //_DisplayCode([<nStep> is int])
      //
      // Parameters:
      //	nStep (integer): Number of the step
      // Return value:
      // 	None
      //
      PROCEDURE _DisplayCode(LOCAL nStep is int = gnCurrentStep)
      
      // According to the step
      SWITCH nStep
      
      	CASE cstPLANE_MasterReplica
      		EDT_RTFCode = STC_Code_Step_1
      		
      	CASE cstPLANE_SubscriberReplica
      		EDT_RTFCode = STC_Code_Step_2
      		
      	CASE cstPLANE_Transfer
      		EDT_RTFCode = STC_Code_Step_3
      		
      	CASE cstPLANE_MovableReplica
      		EDT_RTFCode = STC_Code_Step_4
      		
      END
     type : 458752
